 w.start_if do |start|     start.condition(:process_running) do |c|       c.interval = 180.seconds       c.running = false       case ENV["WEBKITE_ENV"]       when "staging" then         c.notify = {:contacts => ['DevOps'], :category => ENV['WEBKITE_ENV']}       when "production" then         c.notify = {:contacts => ['Dev', 'OpsGenie'], :category => ENV['WEBKITE_ENV']}       end      end   end
